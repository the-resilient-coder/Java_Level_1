package Set1;

import java.util.Scanner;

public class DataTypeAddition {

	public static void main(String s[]){
		int i = 4;
        double d = 4.0;
        String testString = "HackerRank ";
		
        //12System.out.println("Programs begins");
        
        Scanner scan = new Scanner(System.in);
        
        int j;
        double d2;
        String testString2;
        
        j = scan.nextInt();
        d2 = scan.nextDouble();
        testString2 = scan.next();
       
        System.out.println(i+j);
        System.out.println(d+d2);
        System.out.println(testString+testString2);
        
	}
}
